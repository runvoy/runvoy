# Runvoy Environment Variables Example
# Copy this file to .env and update with your actual values

# AWS Configuration
AWS_REGION=us-east-2
AWS_PROFILE=your-aws-profile

# Development Server Configuration
RUNVOY_DEV_SERVER_PORT=56212
RUNVOY_REQUEST_TIMEOUT=0
RUNVOY_LOG_LEVEL=INFO
RUNVOY_INIT_TIMEOUT=10s

# AWS Resources (required for orchestrator and event processor)
RUNVOY_API_KEYS_TABLE=runvoy-api-keys
RUNVOY_EXECUTIONS_TABLE=runvoy-executions
RUNVOY_ECS_CLUSTER=runvoy-cluster
RUNVOY_TASK_DEFINITION=runvoy-task
RUNVOY_TASK_DEFINITION_WITH_GIT=
RUNVOY_SUBNET_1=subnet-xxxxxxxxxxxxxxxxx
RUNVOY_SUBNET_2=subnet-yyyyyyyyyyyyyyyyy
RUNVOY_SECURITY_GROUP=sg-xxxxxxxxxxxxxxxxx
RUNVOY_LOG_GROUP=/aws/ecs/runvoy

# Docker Image Configuration
RUNVOY_DEFAULT_IMAGE=public.ecr.aws/docker/library/ubuntu:22.04

# Release Bucket Configuration
RUNVOY_RELEASES_BUCKET=runvoy-releases

# Admin Configuration (for seeding admin user)
RUNVOY_ADMIN_EMAIL=admin@example.com

# API Configuration (for smoke tests and local development)
RUNVOY_ADMIN_API_KEY=p_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
RUNVOY_LAMBDA_URL=https://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.lambda-url.us-east-2.on.aws
