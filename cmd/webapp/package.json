{
  "name": "runvoy-webapp",
  "version": "1.0.0",
  "description": "Web-based client for runvoy CLI",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.svelte",
    "lint:fix": "eslint src --ext .js,.svelte --fix",
    "format": "prettier --write \"src/**/*.{js,svelte}\"",
    "format:check": "prettier --check \"src/**/*.{js,svelte}\""
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^4.2.20",
    "svelte-eslint-parser": "^1.4.0",
    "vite": "^5.0.11"
  }
}
